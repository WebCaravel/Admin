{
    "name": "webcaravel/admin",
    "description": "A filament based admin",
    "keywords": [
        "WebCaravel",
        "laravel",
        "admin"
    ],
    "homepage": "https://github.com/webcaravel/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "HÃ¶rmann Bernhard",
            "email": "hoermann.b@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73",
        "filament/forms": "^v2.9",
        "filament/tables": "^2.9",
        "wireui/wireui": "^v0.16.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "WebCaravel\\Admin\\": "src",
            "WebCaravel\\Admin\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebCaravel\\Admin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebCaravel\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "WebCaravel\\Admin\\Facades\\Admin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
